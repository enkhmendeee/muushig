{"ast":null,"code":"import{useState,useCallback}from'react';export const useBotMessage=()=>{const[botActionMessage,setBotActionMessage]=useState('');const showBotMessage=useCallback(function(message){let duration=arguments.length>1&&arguments[1]!==undefined?arguments[1]:3000;setBotActionMessage(message);setTimeout(()=>{setBotActionMessage('');},duration);},[]);const clearBotMessage=useCallback(()=>{setBotActionMessage('');},[]);return{botActionMessage,showBotMessage,clearBotMessage};};","map":{"version":3,"names":["useState","useCallback","useBotMessage","botActionMessage","setBotActionMessage","showBotMessage","message","duration","arguments","length","undefined","setTimeout","clearBotMessage"],"sources":["/Users/enkhmendeee/muushig/client/src/hooks/useBotMessage.ts"],"sourcesContent":["import { useState, useCallback } from 'react';\n\nexport const useBotMessage = () => {\n  const [botActionMessage, setBotActionMessage] = useState<string>('');\n\n  const showBotMessage = useCallback((message: string, duration: number = 3000) => {\n    setBotActionMessage(message);\n    setTimeout(() => {\n      setBotActionMessage('');\n    }, duration);\n  }, []);\n\n  const clearBotMessage = useCallback(() => {\n    setBotActionMessage('');\n  }, []);\n\n  return {\n    botActionMessage,\n    showBotMessage,\n    clearBotMessage\n  };\n};\n"],"mappings":"AAAA,OAASA,QAAQ,CAAEC,WAAW,KAAQ,OAAO,CAE7C,MAAO,MAAM,CAAAC,aAAa,CAAGA,CAAA,GAAM,CACjC,KAAM,CAACC,gBAAgB,CAAEC,mBAAmB,CAAC,CAAGJ,QAAQ,CAAS,EAAE,CAAC,CAEpE,KAAM,CAAAK,cAAc,CAAGJ,WAAW,CAAC,SAACK,OAAe,CAA8B,IAA5B,CAAAC,QAAgB,CAAAC,SAAA,CAAAC,MAAA,IAAAD,SAAA,MAAAE,SAAA,CAAAF,SAAA,IAAG,IAAI,CAC1EJ,mBAAmB,CAACE,OAAO,CAAC,CAC5BK,UAAU,CAAC,IAAM,CACfP,mBAAmB,CAAC,EAAE,CAAC,CACzB,CAAC,CAAEG,QAAQ,CAAC,CACd,CAAC,CAAE,EAAE,CAAC,CAEN,KAAM,CAAAK,eAAe,CAAGX,WAAW,CAAC,IAAM,CACxCG,mBAAmB,CAAC,EAAE,CAAC,CACzB,CAAC,CAAE,EAAE,CAAC,CAEN,MAAO,CACLD,gBAAgB,CAChBE,cAAc,CACdO,eACF,CAAC,CACH,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","extern